generator client {
  provider     = "prisma-client"
  output       = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Character {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  x         Float
  y         Float
  radius    Float    @default(5)
  createdAt DateTime @default(now())
}

model Leaderboard {
  id       Int    @id @default(autoincrement())
  username String
  time     String
}
